{
    "actor": "urn:localhost:tracker",
    "type": "htracker",
    "children": [
        {
            "actor": "urn:localhost:gateway",
            "type": "hgateway",
            "method": "inproc",
            "children": [
                {
                    "actor": "urn:localhost:auth",
                    "type": "hauth",
                    "method": "inproc"
                }
            ],
            "inboundAdapters": [ { "type": "socket", "url": "tcp://127.0.0.1:3993" } ],
            "properties": {
                "socketIOPort": 8080,
                "authActor": "urn:localhost:auth",
                "authTimeout": 3000
            }
        },
        {
            "actor": "urn:localhost:dispatcher",
            "type": "hdispatcher",
            "method": "inproc",
            "workers": { "method": "fork", "type": "hactor", "nb": 2 },
            "inboundAdapters": [ { "type": "socket", "url": "tcp://127.0.0.1:2992" } ]
        },
        {
            "actor": "urn:localhost:channel",
            "type": "hchannel",
            "method": "inproc",
            "properties": {
                "listenOn": "tcp://127.0.0.1:2112",
                "broadcastOn": "tcp://127.0.0.1:8992",
                "subscribers": []
            }
        }
    ],
    "properties":{
        "channel": {
            "actor": "urn:localhost:#trackChannel",
            "type": "hchannel",
            "method": "inproc",
            "properties": {
                "listenOn": "tcp://127.0.0.1:1221",
                "broadcastOn": "tcp://127.0.0.1:2998",
                "subscribers": []
            }
        }
    },
    "inboundAdapters": [ { "type": "socket", "url": "tcp://127.0.0.1:2997" } ]
}